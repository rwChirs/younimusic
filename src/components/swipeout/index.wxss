.p-swipeout-wrap{background:#fff;position:relative;overflow:hidden}
.p-swipeout-item{width:100%;padding:0;box-sizing:border-box;transition:transform .2s ease;font-size:30rpx}
.p-swipeout-content{overflow:hidden}
.p-swipeout-button-right-group{position:absolute;right:-100%;top:0;height:100%;z-index:1;width:100%}
.p-swipeout-button-right-item{height:100%;float:left;white-space:nowrap;box-sizing:border-box;display:flex;align-items:center;justify-content:center}