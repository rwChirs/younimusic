/**index.wxss**/

page {
  background-color: #003040;
}

.page {
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
}

.container {
  padding-bottom: 10rpx;
  background-color: rgb(247, 247, 247);
}

.noOpen {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: stretch;
  box-sizing: border-box;
  width: 700rpx;
  height: 132rpx;
  margin-top: 20rpx;
  border-radius: 12rpx;
  background-color: #fff;
  overflow: hidden;
}

.wxIcon {
  background-color: rgb(0, 181, 15);
  width: 112rpx;
  height: 132rpx;
  background-image: url(https://img30.360buyimg.com/mobilecms/jfs/t20110/303/1778878093/1420/377a3e70/5b3d7748Nf10b0270.png);
  background-size: 50%;
  background-repeat: no-repeat;
  background-position: center;
}

.wxText {
  flex: 1;
  padding: 42rpx 0 42rpx 32rpx;
}

.wxTextTip {
  line-height: 48rpx;
  font-size: 34rpx;
  font-weight: bold;
  display: inline-block;
}
.wxOpenStatus{
  display: inline-block;
  width: 72rpx;
  height: 32rpx;
  background-color: rgb(0, 181, 15);
  border-radius: 6rpx;
  line-height: 32rpx;
  font-size: 20rpx;
  color: #fff;
  text-align: center;
  margin-left: 12rpx;
}
.openText {
  width: 140rpx;
  height: 132rpx;
  margin-right: 30rpx;
  padding: 42rpx 0;
  font-size: 28rpx;
  position: relative;
}
.openTextBtn{
  width: 140rpx;
  height: 48rpx;
  border-radius: 24rpx;
  border: solid 1px rgba(137, 137, 137, 0.6);
  line-height: 48rpx;
  text-align: center;
  font-size: 26rpx;
  color: rgb(138, 138, 138);
}
.openTextBtnActive{
  background-color: rgb(0, 181, 15);
  color: #fff;
  border: none;
}
.tips{
  font-size: 24rpx;
  color: rgb(138, 138, 138);
  padding: 30rpx;
  line-height: 40rpx;
}
.tips-title{

}
.tips-content{

}








.panel {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: stretch;
  box-sizing: border-box;
  width: 700rpx;
  margin-top: 40rpx;
  border-radius: 10rpx;
  background-color: #fff;
}

.header {
  height: 140rpx;
  background-color: #53B63E;
  border-radius: 10rpx 10rpx 0 0;
  line-height: 120rpx;
  padding: 0 30rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  vertical-align: middle;
}

.header-icon {
  width: 58rpx;
  height: 52rpx;
  background-image: url(https://img30.360buyimg.com/mobilecms/jfs/t23701/127/485837426/1202/7c2e00f4/5b31e7ebN59ed773c.png);
  background-size: 58rpx 52rpx;
  background-repeat: no-repeat;
  background-position: center;
  display: inline-block;
  vertical-align: middle;
  line-height: 120rpx;
}

.header-text {
  flex: 1;
  display: inline-block;
  vertical-align: middle;
  font-size: 30rpx;
  color: #fff;
  margin-left: 20rpx;
  text-align: left;
}

.header-right-icon {
  width: 48rpx;
  height: 48rpx;
  background-image: url(https://img30.360buyimg.com/mobilecms/jfs/t21583/271/1735813570/548/f1369607/5b32f785N2605455b.png);
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
  display: inline-block;
  vertical-align: middle;
}

page {
  background-color: rgba(0, 0, 0, 0.03);
}

.page {
  display: flex;
  flex-direction: column;
  /* justify-content: center; */
  align-items: center;
}

.container {
  padding-bottom: 10rpx;
}

.openContainer {
  /* display: flex; */
  flex-direction: column;
  justify-content: space-between;
  align-items: stretch;
  box-sizing: border-box;
  width: 690rpx;
  height: 440rpx;
  margin-top: 30rpx;
  padding: 30rpx;
  border-radius: 15rpx;
  background-color: #fff;
  overflow: hidden;
  border: solid rgba(137, 137, 137, 0.1) 1rpx;
  box-sizing: border-box;
}

.openTop {
  width: 100%;
  height: 170rpx;
  border-bottom: solid 1px rgba(0, 0, 0, 0.1);
  padding-top: 30rpx;
}

.wx-icon {
  width: 98rpx;
  height: 86rpx;
  background-image: url(http://img30.360buyimg.com/mobilecms/jfs/t19873/143/2156032374/3065/84bd3098/5b31fb05Nd5862889.png);
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
  margin-left: 117rpx;
  float: left;
}

.wxText {
  float: left;
  margin-left: 30rpx;
}

.wx-text-top {
  line-height: 43rpx;
}

.wx-text-big {
  display: inline-block;
  font-size: 34rpx;
  line-height: 43rpx;
  font-weight: bold;
  vertical-align: middle;
}

.wx-pay-status {
  display: inline-block;
  width: 93rpx;
  height: 32rpx;
  background-image: url(https://img30.360buyimg.com/mobilecms/jfs/t20728/299/1723801945/1086/4dd8dba3/5b32107bN0e055bf8.png);
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
  margin-left: 10rpx;
  font-size: 22rpx;
  color: #fff;
  line-height: 32rpx;
  text-align: center;
  vertical-align: middle;
}

.wx-pay-status-done {
  background-image: url(https://img30.360buyimg.com/mobilecms/jfs/t20758/77/1716309268/1068/a72896ac/5b3215e8N2ae0df3b.png);
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
}

.wx-text-bottom {
  line-height: 43rpx;
  font-size: 26rpx;
  color: rgb(134, 134, 134);
}

.wxTextBig {
  line-height: 30rpx;
  font-size: 28rpx;
  font-weight: bold;
}

.open-bottom {
  font-size: 24rpx;
  color: rgb(153, 153, 153);
  line-height: 30rpx;
  margin-top: 20rpx;
}

.open-btn {
  width: 190rpx;
  height: 68rpx;
  border-radius: 12rpx;
  border: solid 1px rgb(194, 194, 194);
  line-height: 68rpx;
  text-align: center;
  font-size: 26rpx;
  color: rgb(37, 37, 37);
  margin: 20rpx auto 0 auto;
}

.pay-config{
  width: 690rpx;
  height: auto;
  margin: 20rpx auto 0;
  background: #fff;
  padding: 20px;
}
.pay-config-title{
  font-size: 32rpx;
  color: rgb(37,37,37);
  margin-bottom: 20rpx;
}
.switchs{
  display: flex;
  height: auto;
  margin: 0 auto;
  border-top: 1rpx solid rgba(0, 0, 0, 0.03);
}
.switchs .switch{
  zoom: 0.7;
}
.switch-box{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  width: 690rpx;
  height: 100rpx;
}
.switch-title{
  font-size: 30rpx;
  color: rgb(37,37,37);
}
.switch-btn{
  display: flex;
  align-items: center;
}
.switch-txt{
  font-size: 26rpx;
  color: rgb(134, 134, 134);
  margin-right: 15rpx;
}
