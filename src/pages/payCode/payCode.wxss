.pay-tip-wrap-layer{
  width: 200rpx;
  height: 130rpx;
  background: #ffffff;
  position: absolute;
  right: 20rpx;
  top:140rpx;
  display: flex;
  flex-direction: column;
  border-radius: 10rpx;
  border-bottom: 1px solid rgba(0,0,0,0.05);
}
.pay-tip-wrap-layer-icon{
  position: absolute;
  top:-14rpx;
  right: 40rpx;
  width: 0;
  height: 0;
  border-left: 15rpx solid transparent;
  border-right: 15rpx solid transparent;
  border-bottom: 15rpx solid #ffffff;
}
.pay-tip-txt{
  height: 75px;
  display: flex;
  font-size: 28rpx;
  color: rgb(37,37,37);
  justify-content: center;
  align-items: center;
}
